{
  "name": "@tana/mcp-codemode",
  "version": "0.1.0",
  "description": "MCP server for Tana with codemode execution",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "start": "bun run --env-file=.env src/index.ts",
